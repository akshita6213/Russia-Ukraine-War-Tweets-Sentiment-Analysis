   "source": [
    "from collections import defaultdict\n",
    "\n",
    "def n_gram_model(text, n=2):\n",
    "    '''Generate an N-gram model from the input text.'''\n",
    "    words = text.split()\n",
    "    n_grams = defaultdict(list)\n",
    "    for i in range(len(words) - n + 1):\n",
    "        n_gram = tuple(words[i:i + n])\n",
    "        next_word = words[i + n] if i + n < len(words) else None\n",
    "        n_grams[n_gram].append(next_word)\n",
    "    return dict(n_grams)\n",
    "\n",
    "# Initialize an empty dictionary to store N-gram models for each date\n",
    "n_gram_models = {}\n",
    "\n",
    "# Iterate over each date in the dataset\n",
    "for date in df['date'].unique():\n",
    "    # Concatenate the text data associated with the current date into a single string\n",
    "    text_on_date = ' '.join(df[df['date'] == date]['content'])\n",
    "    \n",
    "    # Apply the N-gram model to the concatenated text\n",
    "    n_gram_models[date] = n_gram_model(text_on_date)\n",
    "\n",
    "# # Print the N-gram models for each date\n",
    "# for date, n_gram_model in n_gram_models.items():\n",
    "#     print(f\"N-gram model for date {date}:\")\n",
    "#     print(n_gram_model)\n",
    "#     print()\n"
   ]

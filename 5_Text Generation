    "# Flatten the lists of next words into a single list of all words generated by the Markov chain dictionary\n",
    "all_words = [word for sublist in markov_dict_on_date.values() for word in sublist]\n",
    "\n",
    "# Convert the list of words into a single string\n",
    "text_data = ' '.join(all_words)\n",
    "\n",

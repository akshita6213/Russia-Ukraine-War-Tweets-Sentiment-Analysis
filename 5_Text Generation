    "# Filter the DataFrame based on the desired date\n",
    "desired_date = '2022-03-05'  # Or any other specific date you're interested in\n",
    "text_on_date = df[df['date'] == desired_date]['content'].str.cat(sep=' ')\n",
    "\n",
    "# Create the Markov chain dictionary for the text on the specific date\n",
    "markov_dict_on_date = markov_chain(text_on_date)\n",
    "\n",
    "\n"

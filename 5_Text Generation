    "def tokenize_text_advanced(text):\n",
    "    '''Tokenize the input text using advanced tokenization techniques.'''\n",
    "    text = re.sub(r'[^\\w\\s\\'-]', '', text)\n",
    "    words = re.findall(r\"\\b\\w+(?:[-']\\w+)*\\b\", text)\n",
    "    return words"
